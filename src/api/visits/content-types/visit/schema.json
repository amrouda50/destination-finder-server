{
  "kind": "collectionType",
  "collectionName": "visits",
  "info": {
    "singularName": "visit",
    "pluralName": "visits",
    "displayName": "visits"
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "minLength": 5
    },
    "description": {
      "type": "text",
      "required": true,
      "minLength": 10
    },
    "review": {
      "type": "integer",
      "min": 0,
      "max": 5,
      "required": true
    },
    "user": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::user-statistics.user-statistics",
      "mappedBy": "visits"
    },
    "images": {
      "type": "media",
      "allowedTypes": [
        "images"
      ],
      "multiple": true
    },
    "region": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::region.region",
      "inversedBy": "visits"
    },
    "arrived": {
      "type": "timestamp",
      "required": true
    },
    "departed": {
      "type": "timestamp",
      "required": true
    }
  }
}
